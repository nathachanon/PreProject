 
@{ 
    Layout = "_Layout"; 
} 
 
<!DOCTYPE html> 
<html lang="en"> 
 
<head> 
    <link rel="stylesheet" type="text/css" src="~/css/sweetalert2.min.css">
    <script src="~/js/sweetalert2.min.js"></script>

  <meta charset="utf-8"> 
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"> 
