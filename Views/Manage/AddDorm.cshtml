<!DOCTYPE html>
<html lang="en">

<head>
<!-- Required meta tags -->
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<title>จัดการหอพัก</title>

</head>

<body>
<div class="container-scroller">
  <!-- partial:partials/_navbar.html -->
  <nav class="navbar default-layout col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
    <div class="text-center navbar-brand-wrapper d-flex align-items-top justify-content-center">
      <a class="navbar-brand brand-logo" href="index.html">
        <img src="~/images/logo.svg" alt="logo" />
      </a>
      <a class="navbar-brand brand-logo-mini" href="index.html">
        <img src="~/images/logo-mini.svg" alt="logo" />
      </a>
    </div>
    <div class="navbar-menu-wrapper d-flex align-items-center">
      <ul class="navbar-nav navbar-nav-left header-links d-none d-md-flex">
        <li class="nav-item">
          <a  class="nav-link "><h4 >Mansion Assistant System</h4>
            <span class="badge badge-primary ml-2">New</span>
          </a>
        </li>

      </ul>
      <ul class="navbar-nav navbar-nav-right">
        
        <li class="nav-item dropdown d-none d-xl-inline-block">
          <a class="nav-link dropdown-toggle" id="UserDropdown" href="#" data-toggle="dropdown" aria-expanded="false">
            <span class="profile-text">Hello, @ViewBag.myName</span>
            <img class="img-xs rounded-circle" src="~/images/faces/face1.jpg" alt="Profile image">
          </a>
          <div class="dropdown-menu dropdown-menu-right navbar-dropdown" aria-labelledby="UserDropdown">
            <a class="dropdown-item p-0">
              <div class="d-flex border-bottom">
                <div class="py-3 px-4 d-flex align-items-center justify-content-center">
                  <i class="mdi mdi-bookmark-plus-outline mr-0 text-gray"></i>
                </div>
                <div class="py-3 px-4 d-flex align-items-center justify-content-center border-left border-right">
                  <i class="mdi mdi-account-outline mr-0 text-gray"></i>
                </div>
                <div class="py-3 px-4 d-flex align-items-center justify-content-center">
                  <i class="mdi mdi-alarm-check mr-0 text-gray"></i>
                </div>
              </div>
            </a>
            <a class="dropdown-item mt-2">
              Manage Accounts
            </a>
            <a class="dropdown-item">
              Change Password
            </a>
            <a class="dropdown-item">
              Check Inbox
            </a>
            <form asp-controller="User" asp-action="Logout" method="POST">
                <button type="submit" class="btn btn-danger btn-fw">Logout</button>
            </form>
          </div>
        </li>
      </ul>
      <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
        <span class="mdi mdi-menu"></span>
      </button>
    </div>
  </nav>
  <!-- partial -->
  <div class="container-fluid page-body-wrapper">
    <!-- partial:partials/_sidebar.html -->
    <nav class="sidebar sidebar-offcanvas" id="sidebar">
      <ul class="nav">
      <li class="nav-item">
          <a class="nav-link"  asp-controller="Manage" asp-action="Main" >
            <i class="menu-icon mdi mdi-home-account"></i>
            <span class="menu-title">หน้าแรก</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link"  asp-controller="Manage" asp-action="ManageDorm" >
            <i class="menu-icon mdi mdi-city"></i>
            <span class="menu-title">จัดการหอ</span>
          </a>
        </li>
        <li class="nav-item">
          <a class="nav-link" data-toggle="collapse" href="#ui-basic" aria-expanded="false" aria-controls="ui-basic">
            <i class="menu-icon mdi mdi-content-copy"></i>
            <span class="menu-title">Basic UI Elements</span>
            <i class="menu-arrow"></i>
          </a>
          <div class="collapse" id="ui-basic">
            <ul class="nav flex-column sub-menu">
              <li class="nav-item">
                <a class="nav-link" href="pages/ui-features/buttons.html">Buttons</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="pages/ui-features/typography.html">Typography</a>
              </li>
            </ul>
          </div>
        </li>
        
      </ul>
    </nav>
    <!-- partial -->
        <div class="main-panel">
        <div class="content-wrapper">
    <div class="col-12 grid-margin">
              <div class="card">
                <div class="card-body">
                 <h3 class="font-weight-medium text-primary text-center mdi mdi-city mdi-60px">กรุณาระบุข้อมูลหอพัก</h3>
                  <form class="form-sample" asp-controller ="Manage" asp-action ="AddDorm" method="POST">
                    <h4 class="card-description text-primary">
                      ข้อมูลหอพัก
                    </h4>
                <div class="row">
                      <div class= "col-md-6">
                            <div class="form-group row">
                              <label class="col-sm-3 col-form-label">ชื่อหอ</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="d_name" required type="text" class="form-control" placeholder="ชื่อหอพัก"  >
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-city mdi-18px text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>
                      <div class="col-md-6">
                          <div class="form-group row">
                              <label class="col-sm-3 col-form-label">อัพโหลดรูปภาพ (null)</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                    <div class="custom-file">
                                      <input name="picture" type="file" class="custom-file-input" id="inputGroupFile01" aria-describedby="inputGroupFileAddon01">
                                      <label class="custom-file-label" for="inputGroupFile01">Choose file</label>
                                    </div>
                               
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>


                  <div class="row">
                        <div class= "col-md-6">
                            <div class="form-group row">
                              <label class="col-sm-3 col-form-label">ระบุชั้น</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="setfloor" required type="text" class="form-control" placeholder="หอมีกี่ชั้น"  aria-describedby="colored-addon3">
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-menu text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>
                      <div class= "col-md-6">
                            <div class="form-group row">
                              <label class="col-sm-3 col-form-label">ระบุห้อง</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="setroom" required type="text" class="form-control" placeholder="จำนวนห้องแต่ละชั้น"  aria-describedby="colored-addon3">
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-menu text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>
                    </div>
                    <div class="row">
                      <div class= "col-md-6">
                            <div class="form-group row">
                              <label class="col-sm-3 col-form-label">ราคาเช่า</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="setrates" required type="text" class="form-control" placeholder="ราคาต่อเดือน"  aria-describedby="colored-addon3">
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-menu text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>
                      <div class= "col-md-6">
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label">ระบุค่าน้ำ (null)</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="setwater" type="text" class="form-control" placeholder="ค่าน้ำต่อยูนิต" aria-describedby="colored-addon3">
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-menu text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>
                    </div>
                    <div class = "row">
                      <div class= "col-md-6">
                            <div class="form-group row">
                                <label class="col-sm-3 col-form-label">ระบุค่าไฟ (null)</label>
                              <div class="col-sm-9">
                                <div class="input-group">
                                  <input name="setelec" type="text" class="form-control" placeholder="ค่าไฟต่อยูนิต"  aria-describedby="colored-addon3">
                                    <div class="input-group-append bg-primary border-primary">
                                      <span class="input-group-text bg-transparent">
                                        <i class="mdi mdi-menu text-white"></i>
                                      </span>
                                    </div>
                                </div>
                             </div>
                          </div>
                        </div>

                    </div>


                    <h4 class="card-description text-primary"> ที่อยู่หอพัก (null) </h4>
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">เลขที่</label>
                          <div class="col-sm-9">
                            <input name="add_no" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">ถนน</label>
                          <div class="col-sm-9">
                            <input name="street" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">ตำบล</label>
                          <div class="col-sm-9">
                            <input name="sub_district" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">อำเภอ</label>
                          <div class="col-sm-9">
                            <input name="district" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">จังหวัด</label>
                          <div class="col-sm-9">
                            <input name="province" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                       <div class="col-md-6">
                        <div class="form-group row">
                          <label class="col-sm-3 col-form-label">ไปรษณีย์</label>
                          <div class="col-sm-9">
                            <input name="zip_code" type="text" class="form-control" />
                          </div>
                        </div>
                      </div>
                    
                    </div>
                   
                    
                    <button type="submit" class="btn btn-success btn-fw item-center">
                    <i class="mdi mdi-check"></i>บันทึก</button>

 

                    

                  </form>
                </div>
              </div>
            </div>
          </div>
        </div>
        </div>
        </div>

          
         
        
      <!-- partial:partials/_footer.html -->
   
      <!-- partial -->
    </div>
    <!-- main-panel ends -->
  </div>
  <!-- page-body-wrapper ends -->
</div>
<!-- container-scroller -->

<!-- plugins:js -->

<!-- End custom js for this page-->
</body>

</html>
