                   });
                                $("#renter_off_@renter.Rid").hide(); 
                                $("#renter_on_@renter.Rid").show(); 
                                $("#checkbox_on_@renter.Rid").prop("checked", false);
                                window.location.href = '/Renter/ShowRoom?did=@ViewBag.did'
                            }
                        }
                    }) 
                } 
            } 
            </script>  
    } 
     
</body> 
